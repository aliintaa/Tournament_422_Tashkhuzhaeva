<Page x:Class="Tournament_422_Tashkhuzhaeva.ParticipantsRegistrationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Tournament_422_Tashkhuzhaeva"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="ParticipantsRegistrationPage">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="7*"/>
            <RowDefinition Height="68*"/>
        </Grid.RowDefinitions>
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" Grid.Row="0">
            <TextBlock HorizontalAlignment="Center" FontSize="20">Зона участников</TextBlock>
        </StackPanel>
        <StackPanel  HorizontalAlignment="Center" Orientation="Horizontal" Grid.Row="1">
            <TextBlock HorizontalAlignment="Center" FontSize="15">Регистрация участинков</TextBlock>
            
        </StackPanel>
    </Grid>
</Page>
